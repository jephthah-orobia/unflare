{
  "name": "unflare",
  "version": "1.2.3",
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "clean": "del-cli dist",
    "build": "npm run clean && tsc",
    "build:first": "tsc",
    "test:ci": "vitest run",
    "it:test": "npm run build && cd integration-testing && chmod u+x testAll.sh && bash ./testAll.sh",
    "test:all": "npm run test:ci & npm run it:test"
  },
  "keywords": [
    "unflare",
    "serverless",
    "cloudflare",
    "workers",
    "routing",
    "router",
    "express-flare",
    "workers routing",
    "cloudflare routing",
    "cloudflare router"
  ],
  "author": "Jephthah M. Orobia <jephthah.orobia@gmail.com> (https://jephthah-orobia.github.io/)",
  "license": "MIT",
  "description": "Just another express-inspired web framework for cloudflare's workers.",
  "repository": {
    "url": "https://github.com/jephthah-orobia/unflare",
    "type": "github",
    "directory": "jephthah-orobia/unflare"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230221.0",
    "@types/mime-types": "^2.1.1",
    "del-cli": "^5.0.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4",
    "wrangler": "^2.10.0"
  },
  "dependencies": {
    "@types/cookie": "^0.5.1",
    "cookie": "^0.5.0"
  }
}
